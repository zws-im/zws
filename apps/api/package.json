	"main": "./dist/src/main.js",
	"scripts": {
		"build": "rm -rf ./dist/ && nest build",
		"clean": "rm -r ./src/ ./scripts/ ./tsconfig.json",
		"lint": "xo",
		"start": "node .",
		"start:debug": "nest start --debug --watch",
		"start:dev": "nest start --watch",
