{
	"author": {
		"email": "jonah@jonah.pw",
		"name": "Jonah Snider",
		"url": "https://jonah.pw"
	},
	"bugs": {
		"url": "https://github.com/zws-im/zws/issues"
	},
	"devDependencies": {
		"@semantic-release/exec": "5.0.0",
		"@tsconfig/node16": "1.0.1",
		"@types/node": "15.3.1",
		"cli-ux": "5.6.2",
		"eslint-plugin-prettier": "3.4.0",
		"prettier": "2.3.2",
		"prettier-config-xo": "1.0.4",
		"prisma": "2.26.0",
		"semantic-release": "17.4.4",
		"semantic-release-docker": "2.2.0",
		"ts-json-schema-generator": "0.94.0",
		"ts-node": "10.0.0",
		"type-fest": "1.2.1",
		"typescript": "4.2.4",
		"xo": "0.40.3"
	},
	"license": "Apache-2.0",
	"main": "./tsc_output/src/index.js",
	"name": "@zws.im/zws",
	"engines": {
		"node": "15.x || 16.x"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zws-im/zws.git"
	},
	"scripts": {
		"build": "tsc",
		"deploy": "semantic-release",
		"lint": "xo",
		"prebuild": "rm -rf tsc_output",
		"style": "prettier --check .",
		"start": "node .",
		"migrations": "prisma migrate deploy",
		"generate-schemas": "node tsc_output/src/generate-schemas"
	},
	"version": "0.0.0-development",
	"xo": {
		"prettier": true,
		"rules": {
			"prettier/prettier": "off",
			"unicorn/prefer-node-protocol": "off",
			"unicorn/filename-case": "off",
			"import/extensions": "off",
			"unicorn/no-array-reduce": "off",
			"unicorn/no-array-callback-reference": "off",
			"unicorn/no-process-exit": "off"
		}
	},
	"dependencies": {
		"@jonahsnider/util": "4.1.0",
		"@prisma/client": "2.26.0",
		"@sentry/node": "6.8.0",
		"consola": "2.15.3",
		"dotenv": "10.0.0",
		"fastify": "3.18.1",
		"fastify-auth": "1.1.0",
		"fastify-bearer-auth": "6.0.0",
		"fastify-error": "0.3.1",
		"fastify-swagger": "4.8.2",
		"millify": "4.0.0",
		"zod": "3.3.4"
	}
}
